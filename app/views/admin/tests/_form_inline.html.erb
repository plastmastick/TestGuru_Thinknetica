<%= form_with model: [:admin, test],
              url: update_inline_admin_test_path(test),
              local: true,
              class: 'form-inline hide row g-2',
              data: { test_id: test.id } do |form|%>

  <div class="col-sm">
    <%= form.text_field :title, class: 'form-control' %>
  </div>

  <div class="col-auto">
    <%= form.submit t('.submit'), class: "btn btn-outline-primary fw-bold" %>
  </div>
<% end %>
